{% comment %}<!--
  Loop through a predefined set of pages and turn them into links in the primary navigation.
  The predefined set of pages is created in _config.yml under the JB namespace
-->{% endcomment %}
{% assign links = site.AK.primary_nav %}

<ul>
{% for link in links %}
  {% if page.url == link[0] %}
    <li class="active">
  {% else %}
    <li>
  {% endif %}
      <a href="{{ link[0] }}">{{ link[1] }}</a>
    </li>
{% endfor %}
</ul>